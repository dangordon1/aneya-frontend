{
  "name": "aneya-frontend",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -p tsconfig.build.json && vite build",
    "preview": "vite preview",
    "vercel-build": "npm run build",
    "test": "vitest",
    "test:run": "vitest run",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "test:unit": "vitest run --exclude='**/*.integration.test.{ts,tsx}'",
    "test:integration": "vitest run src/**/*.integration.test.ts",
    "lint": "eslint src --ext .ts,.tsx",
    "lint:fix": "eslint src --ext .ts,.tsx --fix",
    "typecheck": "tsc --noEmit -p tsconfig.build.json",
    "contracts:sync": "tsx src/scripts/sync-openapi.ts",
    "contracts:test": "vitest run src/contracts/**/*.contract.test.ts",
    "contracts:validate": "npm run contracts:sync && npm run contracts:test"
  },
  "dependencies": {
    "@microsoft/fetch-event-source": "^2.0.1",
    "@react-pdf/renderer": "^4.3.2",
    "@supabase/supabase-js": "^2.86.2",
    "@types/react-calendar": "^3.9.0",
    "firebase": "^12.6.0",
    "lucide-react": "^0.263.1",
    "posthog-js": "^1.305.0",
    "react": "^18.2.0",
    "react-calendar": "^6.0.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.1",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "^25.0.3",
    "@types/react": "^18.2.43",
    "@types/react-dom": "^18.2.17",
    "@typescript-eslint/eslint-plugin": "^8.52.0",
    "@typescript-eslint/parser": "^8.52.0",
    "@vitejs/plugin-react": "^4.2.1",
    "@vitest/coverage-v8": "^4.0.16",
    "@vitest/ui": "^4.0.16",
    "autoprefixer": "^10.4.16",
    "eslint": "^9.39.2",
    "jsdom": "^27.4.0",
    "msw": "^2.12.7",
    "openapi-zod-client": "^1.18.3",
    "postcss": "^8.4.32",
    "tailwindcss": "^3.4.0",
    "tsx": "^4.21.0",
    "typescript": "^5.2.2",
    "vite": "^5.0.8",
    "vitest": "^4.0.16",
    "zod": "^3.25.76"
  }
}
